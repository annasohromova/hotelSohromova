CREATE PROCEDURE lisaGuest
@fname varchar(100),
@lname varchar(100),
@sinse date
as
begin
insert into guest(first_name, last_name, member_since)
values(@fname, @lname, @sinse);
select * from guest;
select * from logi;
end;
--käivitamine
exec lisaGuest 'Anna', 'Sohromova', '2006-02-21';

--Kustutamine ja SELECT
CREATE PROCEDURE deleteGuest
@id int
as 
begin
select * from guest;
delete from guest
where @id=id;
select * from guest;
select * from logi;
end;
--käivitamine
exec deleteGuest 1;Ss
